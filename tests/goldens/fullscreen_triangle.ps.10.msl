#include <metal_stdlib>
#include <simd/simd.h>

using namespace metal;

struct PSMain_out
{
    float4 _entryPointOutput [[color(0)]];
};

fragment PSMain_out PSMain(float4 gl_FragCoord [[position]])
{
    PSMain_out out = {};
    out._entryPointOutput = (gl_FragCoord * 0.5) + float4(0.5);
    return out;
}

