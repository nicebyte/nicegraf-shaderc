#include <metal_stdlib>
#include <simd/simd.h>

using namespace metal;

struct PSMain_out
{
    float4 _entryPointOutput [[color(0)]];
};

struct PSMain_in
{
    float2 ps_in_texcoord [[user(locn0)]];
};

fragment PSMain_out PSMain(PSMain_in in [[stage_in]], texture2d<float> tex2 [[texture(0)]], sampler samp [[sampler(0)]], float4 gl_FragCoord [[position]])
{
    PSMain_out out = {};
    out._entryPointOutput = tex2.sample(samp, (in.ps_in_texcoord * 2.0));
    return out;
}

